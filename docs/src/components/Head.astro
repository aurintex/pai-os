---
// Override Starlight Head to add Vercel Analytics via a client-only component
// (inline "import from '@vercel/analytics'" in head config fails in the browser)
import AnalyticsInit from './AnalyticsInit';
const { head } = Astro.locals.starlightRoute;
---
{head.map(({ tag: Tag, attrs, content }) => <Tag {...attrs} set:html={content} />)}
<AnalyticsInit client:only="react" />
